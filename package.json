{
    "name": "riza-ui",
    "version": "1.0.6",
    "description": "riza-ui",
    "type": "module",
    "global": "./css/main.css",
    "scripts": {
        "test": "echo Ok",
        "changes": "rose :changes",
        "build-common": "rose src/compile.fn src/common.src.css src/common.css",
        "build:global": "parcel build --target global",
        "build": "shx rm -rf .parcel-cache && shx rm -rf css && shx mkdir css && pnpm build-common && pnpm build:global",
        "dev": "parcel serve dev/index.html"
    },
    "targets": {
        "global": {
            "source": "./src/main.css",
            "optimize": true,
            "includeNodeModules": true,
            "isLibrary": true,
            "sourceMap": false
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rsthn/riza-ui.git"
    },
    "author": "RedStar Technologies",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rsthn/riza-ui/issues"
    },
    "homepage": "https://github.com/rsthn/riza-ui#readme",
    "files": [
        "css/",
        "LICENSE",
        "README.md"
    ],
    "devDependencies": {
        "@babel/core": "^7.28.6",
        "babel-plugin-riza": "^2.1.2",
        "parcel": "^2.16.3",
        "riza": "^3.0.43",
        "shx": "^0.3.4"
    },
    "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48"
}
